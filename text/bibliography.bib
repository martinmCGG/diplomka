	file = {arXiv\:1505.00880 PDF:C\:\\Users\\miros\\Zotero\\storage\\ZZZ5NYW9\\Su et al. - 2015 - Multi-view Convolutional Neural Networks for 3D Sh.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\miros\\Zotero\\storage\\YL49TX8E\\1505.html:text/html}
